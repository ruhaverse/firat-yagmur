# ShareUpTime Frontend - Environment Configuration

# API Configuration
# Production API (shared with mobile app)
REACT_APP_API_URL=https://www.shareuptime.com

# Development (if running backend locally)
# REACT_APP_API_URL=http://localhost:8080

# Staging
# REACT_APP_API_URL=https://staging.shareuptime.com

# WebSocket Configuration
REACT_APP_WS_URL=wss://www.shareuptime.com

# App Configuration
REACT_APP_NAME=ShareUpTime
REACT_APP_VERSION=1.0.0

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Build Configuration
GENERATE_SOURCEMAP=false
